msgid "plugins.generic.allowedUploads.displayName"
msgstr "Allowed Uploads Plugin"

msgid "plugins.generic.allowedUploads.description"
msgstr "Allowed Uploads plugin enables to choose the filetypes that are allowed when submitting a manuscript. NOTE! This is not a security plugin! Please make sure that the files directory is outside the web root."

msgid "plugins.generic.allowedUploads.error"
msgstr "Only the following file extensions are allowed: {$allowedExtensions}."

msgid "plugins.generic.allowedUploads.manager.settings.description"
msgstr "Add a semicolon separated list of allowed file extensions. For example doc; docx; pdf; gif; jpg and click OK."

msgid "plugins.generic.allowedUploads.manager.settings.allowedExtensions"
msgstr "Allowed file extensions"
